<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
		
	<typeAliases>
			<typeAlias type="kr.ac.kopo.model.Tour" alias="Tour"/>
			<typeAlias type="kr.ac.kopo.model.Food" alias="Food"/>
			<typeAlias type="kr.ac.kopo.model.Festival" alias="Festival"/>
		 	<typeAlias type="kr.ac.kopo.model.Member" alias="Member"/>
<!--		<typeAlias type="kr.ac.kopo.gameshop.model.Attach" alias="Attach"/>-->
	</typeAliases>
	
	<mappers>
		<mapper resource="mybatis/tour.xml"/>
		<mapper resource="mybatis/food.xml"/>
		<mapper resource="mybatis/festival.xml"/>
		<mapper resource="mybatis/member.xml"/>
<!--		<mapper resource="mybatis/attach.xml"/>-->
	</mappers>
</configuration>
